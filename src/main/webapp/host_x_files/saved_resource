$(function(){
	var time=5,t_s=document.getElementById('t_s');
	var t=setInterval(function(){
	    if (--time == 0) { clearInterval(t); window.location.href = '/'; }
		else{
			if(time<5){time=time}
			t_s.innerHTML=time;
			}
	},1000)
})
